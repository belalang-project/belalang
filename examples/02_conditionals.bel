# Variable declaration and assignment
price := 50;

# Conditional statement example
if price < 30 {
    print("very cheap");
} else if (price < 40) {
    print("not so cheap");
} else {
    print("very not cheap");
}

# Using if as an expression
status := if price < 30 {
    "very cheap"
} else if price < 40 {
    "not so cheap"
} else {
    "very not cheap"
};

# Print the status
print(status);
